<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @group.name %>
</p>

<p>
  <b>Members:</b>
  <ul>
    <% @group.users.each do |user| %>
      <li><%= user.name.presence or user.uid %></li>
    <% end %>
  </ul>
</p>

<%= link_to 'Edit', edit_group_path(@group) %> |
<%= link_to 'Back', groups_path %>
